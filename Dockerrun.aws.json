{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "client",
            "image": "shannerterman/multi-client",
            "hostname": "client",
            "essential": false,
        },
        {
            "name": "server",
            "image": "shannerterman/multi-server",
            "hostname": "api",
            "essential": false,
        },
        {
            "name": "worker",
            "image": "shannerterman/multi-worker",
            "hostname": "worker",
            "essential": false,
        },
        {
            "name": "nginx",
            "image": "shannerterman/multi-nginx",
            "hostname": "nginx",
            "essential": true,
        }
    ]

}